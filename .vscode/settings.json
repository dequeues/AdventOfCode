{
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "python.analysis.extraPaths": [
    "./aoc",
    "./aoc/2020"
  ],
  "python.linting.pycodestyleEnabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--disable", "E0401,C0114,C0116"
  ],
  "python.linting.pylintCategorySeverity.convention": "Error",
  "python.linting.flake8CategorySeverity.F": "Error",
  "python.linting.flake8CategorySeverity.W": "Error",
  "python.linting.pycodestyleCategorySeverity.E": "Error",
  "python.linting.pycodestyleCategorySeverity.W": "Error"
}
